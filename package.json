{
  "name": "talextouchqq",
  "version": "3.0.0-Lite",
  "description": "A third qq client",
  "author": "Tds <2418876761@qq.com>",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "dist": "electron-packager . TouchQ --out application",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "index.js",
  "dependencies": {
    "ant-design-vue": "^1.7.8",
    "axios": "^0.18.0",
    "cors": "^2.8.5",
    "electron-json-storage": "^4.5.0",
    "element-ui": "^2.15.6",
    "jsqr": "^1.4.0",
    "koa": "^2.13.3",
    "koa-bodyparser": "^4.3.0",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "nedb": "^1.8.0",
    "oicq": "^2.1.4",
    "pngjs": "^6.0.0",
    "probe-image-size": "^7.2.1",
    "qrcode-terminal": "^0.12.0",
    "shell": "^0.8.4",
    "v-viewer": "^1.6.4",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuescroll": "^4.17.3",
    "vuetify": "^2.4.0",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^15.1.0",
    "electron-builder": "^22.13.1",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-updater": "^4.3.9",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "koishi": "^3.14.2",
    "koishi-adapter-onebot": "^3.1.0",
    "koishi-core": "^3.14.2",
    "koishi-plugin-common": "^4.3.5",
    "listr": "^0.14.3",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "style-loader": "^0.21.0",
    "thread-loader": "^3.0.4",
    "tributejs": "^5.1.3",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3",
    "url-loader": "^1.0.1",
    "vue-cli-plugin-vuetify": "^2.4.3",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vuetify-loader": "^1.7.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "build": {
    "productName": "TouchQ",
    "appId": "me.tds.touchqq",
    "copyright": "Copyright Â© 2021 TalexDreamSoul",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraResources": {
      "from": "src",
      "to": "dist/electron/src"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "./src/renderer/assets/logo.png"
    },
    "win": {
      "icon": "./src/renderer/assets/logo.png"
    },
    "linux": {
      "icon": "./src/renderer/assets/logo.png"
    },
    "nsis": {
      "oneClick": false,
      "guid": "talextouchqq",
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE",
      "installerIcon": "./src/renderer/assets/logo.ico",
      "uninstallerIcon": "./src/renderer/assets/logo.ico",
      "installerHeaderIcon": "./src/renderer/assets/logo.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "touchq"
    }
  },
  "homepage": "https://github.com/TalexDreamSoul/touchq",
  "license": "GNU Affero General Public License v3.0"
}
